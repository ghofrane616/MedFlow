{
  "files.exclude": {
    "**/node_modules": true,
    "**/__pycache__": true,
    "**/.pytest_cache": true,
    "**/my_env": true,
    "**/venv": true,
    "**/.venv": true,
    "**/env": true,
    "**/*.pyc": true,
    "**/.cache": true,
    "**/build": true,
    "**/dist": true,
    "**/.git": false
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/__pycache__/**": true,
    "**/my_env/**": true,
    "**/venv/**": true,
    "**/.venv/**": true,
    "**/env/**": true,
    "**/.cache/**": true,
    "**/build/**": true,
    "**/dist/**": true,
    "**/*.pyc": true,
    "**/package-lock.json": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/__pycache__": true,
    "**/my_env": true,
    "**/venv": true,
    "**/.venv": true,
    "**/env": true,
    "**/.cache": true,
    "**/build": true,
    "**/dist": true
  },
  "git.ignoreLimitWarning": true,
  "git.autofetch": false,
  "git.autorefresh": false,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "python.analysis.exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/my_env",
    "**/venv",
    "**/.venv",
    "**/env"
  ],
  "typescript.tsserver.maxTsServerMemory": 4096,
  "typescript.disableAutomaticTypeAcquisition": true,
  "javascript.suggest.enabled": true,
  "typescript.suggest.enabled": true,
  "extensions.ignoreRecommendations": true
}

